# Example Environment Configuration
# Copy this file to .env.development or .env.production and fill in your values

NODE_ENV=development
PORT=3000

# SAML Configuration
SAML_CALLBACK_URL=http://localhost:3000/api/auth/callback
SAML_ISSUER=ai-transformation-web
SAML_ENTRYPOINT=https://your-idp-domain.com/sso/saml
SAML_IDP_CERT_PATH=./apps/web/certs/idp_cert.pem

# Session Configuration
SESSION_SECRET=your_session_secret_here

# Database
DATABASE_URL=file:./dev.db

# AI Service
PY_AI_BASE_URL=http://127.0.0.1:8001
AI_PORT=8001
